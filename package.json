    "fix:es": "eslint --fix --ext .js,.vue src",
    "fix:css": "stylelint --fix src/**/*.scss --syntax scss",
    "fix": "run-p fix:css fix:es",
    "deploy:360buy": "npm run build && gulp deploy",
    "deploy:shop": "npm run buildshop && gulp deploy",
    "deploy": "npm run buildall && gulp deploy"
  },
